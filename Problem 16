//Take two numbers from user and determine the largest number

#include<iostream>
using namespace std;

int main()
{
    int a,b;

    cout<<"Enter first number : ";
    cin>>a;
    cout<<"\nEnter second number : ";
    cin>>b;

    if(a>b)
     cout<<"\n"<<a<<" is largest number.";
    else
     cout<<"\n"<<b<<" is largest number.";


    return 0;  
}
